    "import dill\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "from pathlib import Path\n",
    "import sys\n",
    "project_root = Path.cwd().parent\n",
    "sys.path.append(str(project_root))\n",
    "\n",
    "import src.unimib_snowit_project.utils as u"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Setup\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Constants\n",
    "DATA_PKL_DIR = 'data_loaded'\n",
    "USERS_PKL_FILENAME = 'users.pkl'\n",
    "PROFILES_PKL_FILENAME = 'profiles.pkl'\n",
    "CARDS_PKL_FILENAME = 'cards.pkl'\n",
    "ORDERS_PKL_FILENAME = 'orders.pkl'\n",
    "ORDER_DETAILS_PKL_FILENAME = 'order_details.pkl'\n",
    "\n",
    "# File paths\n",
    "root_dir_path = u.get_root_dir()\n",
    "data_pkl_dir_path = root_dir_path.joinpath(DATA_PKL_DIR)\n",
    "\n",
    "orders_pkl_path = data_pkl_dir_path.joinpath(ORDERS_PKL_FILENAME)\n",
    "order_details_pkl_path = data_pkl_dir_path.joinpath(ORDER_DETAILS_PKL_FILENAME)\n",
    "\n",
    "# Load data\n",
    "with orders_pkl_path.open('rb') as f:\n",
    "    orders_df = dill.load(f)\n",
    "\n",
    "with order_details_pkl_path.open('rb') as f:\n",
    "    order_details_df = dill.load(f)\n",
    "\n",
    "# Join and filter successful purchases\n",
    "df = orders_df.merge(order_details_df, on='order.uid')\n",
    "df = df[df['item.status'].isin(['ok', 'fulfilled'])]\n",
    "\n",
    "# Define reference date\n",
    "df['createdAt'] = pd.to_datetime(df['createdAt'])\n",
    "reference_date = df['createdAt'].max()\n"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Compute RFM metrics\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "rfm = df.groupby('user.uid').agg({\n",
    "    'createdAt': lambda x: (reference_date - x.max()).days,  # Recency\n",
    "    'order.uid': 'nunique',                                  # Frequency\n",
    "    'item.amount': 'sum'                                     # Monetary\n",
    "}).reset_index()\n",
    "\n",
    "rfm.columns = ['user.uid', 'Recency', 'Frequency', 'Monetary']"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Define inactive customers\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Sort and calculate inter-purchase times\n",
    "orders_df = orders_df.sort_values(['user.uid', 'createdAtTime'])\n",
    "\n",
    "# Create a new column with previous purchase datetime\n",
    "orders_df['prev_purchase_time'] = orders_df.groupby('user.uid')['createdAtTime'].shift(1)\n",
    "\n",
    "# Calculate difference in days between purchases\n",
    "orders_df['days_since_last_purchase'] = (\n",
    "    orders_df['createdAtTime'] - orders_df['prev_purchase_time']\n",
    ").dt.days\n",
    "\n",
    "# Drop the first purchase per user (NaN intervals)\n",
    "intervals = orders_df['days_since_last_purchase'].dropna()\n",
    "\n",
    "# Remove weird intervals (same-day, negative, or too large)\n",
    "intervals = intervals[intervals > 0]  # remove 0 or negative\n",
    "intervals = intervals[intervals < 365]  # remove outliers beyond 1 year"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### Plot ECDF\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1000x500 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "plt.figure(figsize=(10, 5))\n",
    "sns.ecdfplot(intervals, stat='proportion')\n",
    "plt.xlabel('Days since last purchase')\n",
    "plt.ylabel('Cumulative proportion of repurchases')\n",
    "plt.title('Repurchase Curve (ECDF)')\n",
    "plt.grid(True)\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### Calculate thresholds\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "80% of customers repurchase within 94 days\n",
      "90% of customers repurchase within 314 days\n",
      "Suggested inactive threshold: 94 days\n"
     ]
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 600x600 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "threshold_80 = intervals.quantile(0.80)\n",
    "threshold_90 = intervals.quantile(0.90)\n",
    "\n",
    "print(f\"80% of customers repurchase within {int(threshold_80)} days\")\n",
    "print(f\"90% of customers repurchase within {int(threshold_90)} days\")\n",
    "\n",
    "# Define inactive threshold suggestion\n",
    "inactive_threshold = int(threshold_80)\n",
    "print(f\"Suggested inactive threshold: {inactive_threshold} days\")\n",
    "\n",
    "rfm['Status'] = rfm['Recency'].apply(lambda x: 'Inactive' if x > inactive_threshold else 'Active')\n",
    "\n",
    "status_counts = rfm['Status'].value_counts()\n",
    "\n",
    "plt.figure(figsize=(6, 6))\n",
    "plt.pie(\n",
    "    status_counts, \n",
    "    labels=status_counts.index, \n",
    "    autopct='%1.1f%%', \n",
    "    startangle=90, \n",
    "    colors=['#4CAF50', '#FF5722']\n",
    ")\n",
    "plt.title('Active vs Inactive Customers')\n",
    "plt.axis('equal')  # Equal aspect ratio ensures that pie is drawn as a circle.\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Bin RFM components (Active only)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "active_rfm = rfm[rfm['Status'] == 'Active'].copy()\n",
    "\n",
    "# Recency\n",
    "active_rfm['R_score'] = pd.qcut(active_rfm['Recency'], q=3, labels=[3, 2, 1], duplicates='drop').astype(int)\n",
    "\n",
    "\n",
    "# Frequency\n",
    "try:\n",
    "    freq_qcut, freq_bins = pd.qcut(active_rfm['Frequency'], q=3, retbins=True, duplicates='drop')\n",
    "    num_bins = len(freq_bins) - 1  # Number of actual bins\n",
    "    active_rfm['F_score'] = pd.qcut(\n",
    "        active_rfm['Frequency'], \n",
    "        q=num_bins, \n",
    "        labels=list(range(1, num_bins + 1)), \n",
    "        duplicates='drop'\n",
    "    ).astype(int)\n",
    "except ValueError as e:\n",
    "    print(\"Could not compute F_score due to:\", e)\n",
    "\n",
    "# Monetary\n",
    "active_rfm['M_score'] = pd.qcut(active_rfm['Monetary'], q=3, labels=[1, 2, 3], duplicates='drop').astype(int)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## RF Matrix\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [],
   "source": [
    "def rf_class(row):\n",
    "    if row['R_score'] == 3 and row['F_score'] == 3:\n",
    "        return 'Top'\n",
    "    elif row['R_score'] >= 2 and row['F_score'] >= 2:\n",
    "        return 'Engaged'\n",
    "    elif row['R_score'] == 1:\n",
    "        return 'Leaving'\n",
    "    elif row['F_score'] == 1:\n",
    "        return 'One-timer'\n",
    "    else:\n",
    "        return 'Leaving Top'\n",
    "\n",
    "active_rfm['RF_class'] = active_rfm.apply(rf_class, axis=1)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# RFM Matrix. Segment assignment"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "def rfm_segment(row):\n",
    "    if row['RF_class'] == 'Top' and row['M_score'] == 3:\n",
    "        return 'Diamond'\n",
    "    elif row['RF_class'] in ['Top', 'Engaged'] and row['M_score'] == 2:\n",
    "        return 'Gold'\n",
    "    elif row['RF_class'] in ['Engaged', 'Leaving Top'] and row['M_score'] == 2:\n",
    "        return 'Silver'\n",
    "    elif row['M_score'] == 1:\n",
    "        return 'Bronze'\n",
    "    elif row['RF_class'] == 'One-timer':\n",
    "        return 'Tin'\n",
    "    else:\n",
    "        return 'Copper'\n",
    "\n",
    "active_rfm['RFM_segment'] = active_rfm.apply(rfm_segment, axis=1)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
